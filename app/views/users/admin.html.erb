<div class="col s12">
  <h2>User</h2>
  <ul>
    <% @users.each do |user| %>
    <li><%= user.id %>:<%= user.name %>:<%= user.email %><%= link_to '削除', user, method: :delete, data: { confirm: '本当に削除してよろしいですか？' }, class: "waves-effect waves-light btn" %></li>
    <% end %>
  </ul>
  <h2>クラシファイド</h2>
  <ul>
    <% @classifieds.each do |classified| %>
    <li><%= classified.id %>:<%= classified.title %><%= link_to '削除', classified, method: :delete, data: { confirm: '本当に削除してよろしいですか？' }, class: "waves-effect waves-light btn" %></li>
    <% end %>
  </ul>
</div>